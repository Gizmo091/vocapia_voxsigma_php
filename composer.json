{
    "name": "vocapia/voxsigma",
    "description": "PHP SDK for Vocapia VoxSigma speech-to-text binaries and REST API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "vocapia",
        "voxsigma",
        "speech-to-text",
        "transcription",
        "stt",
        "asr"
    ],
    "authors": [
        {
            "name": "Mathieu VEDIE LAUTE",
            "email": "mathieu.vedie@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Vocapia\\Voxsigma\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vocapia\\Voxsigma\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
